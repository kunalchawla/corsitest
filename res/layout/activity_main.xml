<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background = "@android:color/white"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity" >

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageButton1"
        android:layout_marginBottom="23dp"
        android:layout_marginLeft="50dp"
        android:layout_toRightOf="@+id/imageButton1"
        android:padding="@dimen/nopadding"
        android:src="@drawable/corsisquare" />

    <ImageButton
        android:id="@+id/imageButton4"
        style="@dimen/activity_horizontal_margin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButton6"
        android:layout_marginLeft="48dp"
        android:layout_toRightOf="@+id/imageButton3"
        android:padding="@dimen/nopadding"
        android:src="@drawable/corsisquare" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButton4"
        android:layout_marginLeft="42dp"
        android:layout_toRightOf="@+id/imageButton2"
        android:padding="@dimen/nopadding"
        android:src="@drawable/corsisquare" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_marginTop="30dp"
        android:layout_toLeftOf="@+id/imageButton4"
        android:padding="@dimen/nopadding"
        android:src="@drawable/corsisquare" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/imageButton4"
        android:layout_below="@+id/imageButton5"
        android:layout_marginRight="21dp"
        android:padding="@dimen/nopadding"
        android:src="@drawable/corsisquare" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton7"
        android:layout_toLeftOf="@+id/imageButton3"
        android:padding="@dimen/nopadding"
        android:src="@drawable/corsisquare" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageButton1"
        android:layout_below="@+id/imageButton6"
        android:layout_marginLeft="23dp"
        android:padding="@dimen/nopadding"
        android:src="@drawable/corsisquare" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/imageButton3"
        android:layout_marginLeft="314dp"
        android:padding="@dimen/nopadding"
        android:src="@drawable/corsisquare" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/imageButton2"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/imageButton7"
        android:padding="@dimen/nopadding"
        android:src="@drawable/corsisquare"
        android:background = "@android:color/white" />

</RelativeLayout>